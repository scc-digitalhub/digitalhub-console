    "scripts": {
        "start": "env-cmd -f ./envs/.env.development vite",
        "start:localhost": "env-cmd -f ./envs/.env.localhost vite",
        "start:development": "env-cmd -f ./envs/.env.development vite",
        "start:production": "env-cmd -f ./envs/.env.production vite --base=/console",
        "build": "env-cmd -f ./envs/.env.development vite build",
        "build:localhost": "env-cmd -f ./envs/.env.localhost vite build",
        "build:production": "env-cmd -f ./envs/.env.production vite build --base=/console",
        "serve": "env-cmd -f ./envs/.env.development vite preview",
        "serve:localhost": "env-cmd -f ./envs/.env.localhost vite preview",
        "serve:production": "env-cmd -f ./envs/.env.production vite preview --base=/console",
        "type-check": "tsc --noEmit",
        "prettier": "prettier --config ./prettier.config.js --write --list-different \"src/**/*.{ts,tsx,js,html}\" ./index.html",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2",
        "i18n-generator-en": "curl -s https://raw.githubusercontent.com/scc-digitalhub/digitalhub-core/refs/heads/main/I18N_KEYS.txt | sh -c ./i18n/i18n-generator.sh > ./src/i18n/en/autogenerated.json",
        "i18n-generator-it": "curl -s https://raw.githubusercontent.com/scc-digitalhub/digitalhub-core/refs/heads/main/I18N_KEYS.txt | sh -c ./i18n/i18n-generator.sh > ./src/i18n/it/autogenerated.json",
        "i18n-missing-keys-fields-en": "cat ./src/i18n/en/fields.json | jq -r '. as $root | path(..) | . as $path | $root | getpath($path) as $value | select($value | scalars) | ([$path[]] | join(\".\"))' | sort -u | sed 's/^/fields./'",
        "i18n-missing-keys-specs-en": "cat ./src/i18n/en/specs.json | jq -r '. as $root | path(..) | . as $path | $root | getpath($path) as $value | select($value | scalars) | ([$path[]] | join(\".\"))' | sort -u | sed 's/^/specs./'"
    },
